<p id="notice"><%= notice %></p>

<div class="content">
  <h1>Listing Products</h1>

  <div class="container">
    <div class="row">
      <% cache(cache_key_for_products) do %>
        <%= render @products %>
      <% end %>
    </div>
  </div>
